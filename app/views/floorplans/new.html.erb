<%
@title = "Add Floorplan"
@back_url = place_floorplans_path
%>

<div class="card">
  <%= form_with url: place_floorplans_path, model: @floorplan do |f| %>
    <div class="mb-3">
      <%= f.label :name, { class: "form-label" } %>
      <%= f.text_field :name, class: "form-control" %>
    </div>

    <%= f.submit "Add", class: "btn btn-success" %>
  <% end %>
</div>
