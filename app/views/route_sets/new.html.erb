<%
@title = "Add Route Set"
@back_url = route_sets_path
%>

<%= form_with url: route_sets_path, model: @route_set do |form| %>
  <%= form.select "Colour", [["Green", "green"], ["White", "white"], ["Blue", "blue"], ["Black", "black"], ["Red", "red"], ["Pink", "pink"]] %>
  <%= form.date_field "Added", value: Time.now.strftime('%Y-%m-%d') %>
  <%= form.submit %>
<% end %>
